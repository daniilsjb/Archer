var foo = [10, 13, 8];

print foo[0] += 5;  //Expected: 15
print foo[0];       //Expected: 15

print foo[0] -= 5;  //Expected: 10
print foo[0];       //Expected: 10

print foo[0] *= 3;  //Expected: 30
print foo[0];       //Expected: 30

print foo[0] /= 2;  //Expected: 15
print foo[0];       //Expected: 15

print foo[0] %= 4;  //Expected: 3
print foo[0];       //Expected: 3

print foo[0] **= 3; //Expected: 27
print foo[0];       //Expected: 27

print foo[0] |= 4;  //Expected: 31
print foo[0];       //Expected: 31

print foo[0] &= 48; //Expected: 16
print foo[0];       //Expected: 16

print foo[0] <<= 1; //Expected: 32
print foo[0];       //Expected: 32

print foo[0] >>= 2; //Expected: 8
print foo[0];       //Expected: 8
