fun fib(n) {
    if (n <= 1) {
        return n;
    }
    
    return fib(n - 1) + fib(n - 2);
}

print fib(0); //Expected: 0
print fib(1); //Expected: 1
print fib(2); //Expected: 1
print fib(3); //Expected: 2
print fib(4); //Expected: 3
print fib(5); //Expected: 5
print fib(6); //Expected: 8
print fib(7); //Expected: 13
print fib(8); //Expected: 21
print fib(9); //Expected: 34